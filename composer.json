{
  "name": "heypongo/laravel-deprecation-middleware",
  "description": "A Laravel middleware to signal HTTP route deprecation (Deprecation, Sunset, Link headers)",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "HeyPongo\\DeprecationMiddleware\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.36",
    "laravel/pint": "^1.20"
  },
  "autoload-dev": {
    "psr-4": {
      "HeyPongo\\DeprecationMiddleware\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HeyPongo\\DeprecationMiddleware\\DeprecationMiddlewareServiceProvider"
      ]
    }
  }
}
